{
  "name": "neetlogiq",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "next dev --turbopack -p 3500",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "echo 'Linting temporarily disabled due to ESLint config issue'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:performance": "node scripts/performance-test.js",
    "generate:embeddings": "node scripts/generate-embeddings.js",
    "build:wasm": "bash scripts/build-wasm.sh",
    "build:rust": "cd build/data-processor && cargo build --release",
    "build:all": "npm run build:wasm && npm run build",
    "create:stream-manifest": "node scripts/create-stream-manifest.js",
    "create:compressed-cutoffs": "node scripts/create-partitioned-cutoffs.js",
    "test:compression": "node scripts/compression-comparison.js",
    "generate:static": "node scripts/generate-static-data.js",
    "upload:data": "node scripts/upload-to-r2.js",
    "deploy:all": "bash scripts/deploy-all.sh",
    "build:optimized": "npm run generate:static && npm run build",
    "migrate:postgres": "tsx scripts/migrate-to-postgres.ts",
    "supabase:types": "supabase gen types typescript --local > src/lib/database.types.ts"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@leeoniya/ufuzzy": "^1.0.19",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "apache-arrow": "^21.1.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "firebase": "^12.4.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "next": "16.0.0",
    "postcss": "^8.5.6",
    "razorpay": "^2.9.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.4.1",
    "simplex-noise": "^4.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "zstd-wasm": "^0.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^17.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/papaparse": "^5.5.0",
    "@vitejs/plugin-react": "^4.4.2",
    "@vitest/ui": "^3.1.6",
    "better-sqlite3": "^12.4.1",
    "duckdb": "^1.4.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "jsdom": "^26.0.0",
    "lz4js": "^0.2.0",
    "natural": "^8.1.0",
    "papaparse": "^5.5.3",
    "sqlite3": "^5.1.7",
    "vitest": "^3.1.6",
    "xlsx": "^0.18.5"
  }
}
